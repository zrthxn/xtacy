<link rel="stylesheet" type="text/css" media="screen" href="/static/css/Events.css">
<link rel="stylesheet" type="text/css" media="screen" href="/static/css/PromoStyle.css">
{{!------ ==== PAGE ==== ------}}

<script>
    var __data;
    window.onload = () => {
        __data = document.getElementById("__data").innerHTML;
    }
</script>

{{!-- START PAGE CONTENT --}}
<section>
    {{#with page}}
        <article class="promo">
            <div class="container">
                <div class="banner small">
                    <a href="#">
                        <img class="desktop" src="/static/img/thumb.png" alt="">
                        <img class="mobile" src="/static/img/JMI Logo.png" alt="">
                    </a>
                </div>

                <div id="metadata" class="metadata">{{ data }}</div>

                <div class="content">{{{ content }}}</div>
            </div>
        </article>

        <div id="__data" hidden="true">{{ data }}</div>
    {{/with}}
</section>
{{!-- END PAGE CONTENT --}}